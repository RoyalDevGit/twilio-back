  "scripts": {
    "dev": "docker-compose up",
    "dev:full": "docker-compose -f docker-compose-full.yml up",
    "dev:container": "yarn install && nodemon",
    "clean": "rimraf dist",
    "build": "yarn clean && ttsc --build",
    "start": "node dist/src/index.js",
    "start:debug": "yarn build && node --inspect=0.0.0.0:9222 dist/src/index.js",
    "daemon": "node dist/src/daemon/index.js",
    "daemon:run": "node dist/src/daemon/startCronJobs.js",
    "daemon:debug": "yarn build && node --inspect=0.0.0.0:9223 dist/src/daemon/startCronJobs.js",
    "daemon:container": "yarn install && nodemon --config nodemon-daemon.json",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "precommit": "tsc && lint-staged",
    "lint-staged": "lint-staged",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .js --ext .ts --cache --fix",
    "lint:check": "eslint . --ext .js --ext .ts --cache",
    "snyk": "synk test; snyk code test"
  },
